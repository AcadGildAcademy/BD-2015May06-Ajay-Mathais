package except;

public class StringIndexOutOfBound {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String hi = "Hello World";
		try{
			System.out.println(hi[100]);
		}
		catch(StringIndexOutOfBound ex){
			System.out.println("");
		}

	}

}
