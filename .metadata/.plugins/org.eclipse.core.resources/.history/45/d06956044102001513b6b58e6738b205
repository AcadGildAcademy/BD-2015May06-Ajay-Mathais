package challenge2;

public class PrintPattern {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 5, i = 0;
		while(i <= 5){
			for(int j = 1; j <= i; j++)
				System.out.print(j);
			System.out.println();
			i++;
		}
		while(i >= 1){
			for(int j = i; j >= 1; j--)
				System.out.print(j);
			System.out.println();
			i--;
		}
	}

}
