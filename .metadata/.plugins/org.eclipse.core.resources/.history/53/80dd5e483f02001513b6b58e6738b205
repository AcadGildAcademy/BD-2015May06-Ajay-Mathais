package challenge1;

import java.util.Scanner;

public class Prime {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		// 1 is neither prime nor composite
		
		Scanner in = new Scanner(System.in);
		int num = in.nextInt();
		for(int i = 2; i <= 100 ; i++)
			if(isPrime(i))
				System.out.println(i + " ");
		
	}
	
	static boolean isPrime(int n){
		for(int i = 2; i < (n / 2); i++)
			if(n % i == 0)
				return false;
		return true;
	}

}
