package challenge1;

public class Prime {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}
	
	static boolean isPrime(int n){
		for(int i = 2; i < (n / 2); i++)
			if(n % i == 0)
				return false;
		return true;
	}

}
